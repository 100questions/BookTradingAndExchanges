<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#407d8b" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#DD000000" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#407d8b" po:Freeze="True"/>
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFFFFF" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#407d8b" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFFFF" po:Freeze="True" />
    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="#407d8b" po:Freeze="True" />
    <SolidColorBrush x:Key="SecondaryAccentForegroundBrush" Color="#DD000000" po:Freeze="True" />
</ResourceDictionary>